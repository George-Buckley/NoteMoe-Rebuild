<ul id="timeline">
    <% unless @notes.blank? %>
    <% @notes.each do |note| %>
      <a href="<%= (url_for [note]) %>">
        <li class="listing clearfix">
          <div class="info">
            <span class="job_title"><%= note.title %></span>
            <span class="job_info">Created <span>-</span> <%= time_ago_in_words(note.created_at) + " ago" %></span>
            <span class="job_links" id="job_links">
            <%= link_to " View " , note_path(note.id), id:"notelinks" %>
            |
            <%= link_to " Edit ", edit_note_path(note.id), id:"notelinks" %>
            |
            <%= link_to " Delete ", note_path(note.id), :method => :delete, id:"notelinks" %>
            </span>
        </div>
        </li>
      </a>
        <% end %>
        <% else %>
          <h2>Add a Note</h2>
          <p>It appears you haven't created any notes yet... Lets fix that. Why don't you go ahead and create a new note.</p>
          <button>
            <%= link_to "New Note", new_note_path %>
          </button>
        <% end %>
    </div>
</div>