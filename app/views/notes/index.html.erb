<ul id="timeline">
  <li class="listing clearfix">
    <div class="info">
    <span class="job_info">Ads:</span><br />
      <div id="startappContainer"></div>
<script type="text/javascript">
// Required parameters.
var publisherId = '103278800';
var productId ='203031383';
var width =728;
var height =90;
// Optional parameters.
var userId = "";
var packageName = "";
var isp = "";
var contentRating = "";
var gender = "";
var age = "";
var category = "";
var networkType = "";
var geoLongitude = "";
var geoLatitude = "";
var subPublisherId = "";
var subProductId = "";
var domain = "";
var deviceManufacturer = "";
var deviceModel = "";
var userAdvertisingId="";
var noAdUrl= "";
</script>
<script type="text/javascript" src="http://www.startappexchange.com/js/startapp-tag.js"></script>
    </div>
  </li>
    <% unless @notes.blank? %>
    <% @notes.each do |note| %>
      <a href="<%= (url_for [note]) %>">
        <li class="listing clearfix">
          <div class="info">
            <span class="job_title"><%= note.title %></span>
            <span class="job_info"><%= time_ago_in_words(note.updated_at) + " ago" %></span>
            <span class="job_links" id="job_links">
            <%= link_to " View " , note_path(note.id), id:"notelinks" %>
            |
            <%= link_to " Edit ", edit_note_path(note.id), id:"notelinks" %>
            |
            <%= link_to " Delete ", note_path(note.id), :method => :delete, id:"notelinks", data: { confirm: 'Delete Note?' }  %>
            </span>
        </div>
        </li>
      </a>
        <% end %>
        <% else %>
        <hr>
          <h2>Add a Note</h2>
          <p>It appears you haven't created any notes yet... Lets fix that. Why don't you go ahead and create a new note.</p>
            <%= link_to "New Note", new_note_path, class: 'btn btn-success' %><br /><hr><br />
        <% end %>
        <center>
        <%= page_entries_info @notes, :model => 'note' %><br />
        <%= will_paginate @notes %>
        </center>
    </div>
</div>