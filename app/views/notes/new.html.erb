<h1>New Note</h1>

<%= form_for @note do |f| %>
    <p>
        <%= f.label :title %><br>
        <%= f.text_field :title %>
    </p>
 
    <p>
        <%= f.label :content %><br>
        <%= f.text_area :content %>
    </p>
 
    <p>
        <%= f.submit %>
    </p>
<% end %>

<%= link_to "Back to Notes", notes_path %>